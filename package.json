{
  "name": "numerator-react-client-sdk",
  "version": "0.0.1",
  "description": "Numerator SDK for ReactJS",
  "main": "lib/index.js",
  "scripts": {
    "clean": "rimraf lib/*",
    "rollup": "rollup -c --configPlugin typescript",
    "rollupw": "yarn rollup --watch",
    "build": "yarn clean && yarn rollup",
    "lint": "tslint -p tsconfig.json 'src/**/*.ts*'",
    "lint:all": "npm run lint",
    "check-typescript": "tsc",
    "prepublishOnly": "npm run build",
    "prettier": "prettier --write 'src/*.@(js|ts|tsx|json|css)'",
    "check": "npm i && npm run prettier && npm run lint && tsc && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/c0x12c/numerator-react-client-sdk.git"
  },
  "keywords": [
    "numerator",
    "react-client-sdk",
    "feature-flags"
  ],
  "author": "Numerator <dev-numerator@c0x12c.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/c0x12c/numerator-react-client-sdk/issues"
  },
  "homepage": "https://github.com/c0x12c/numerator-react-client-sdk#readme",
  "devDependencies": {
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/react": "^18.2.47",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "esbuild": "^0.19.11",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^43.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0 || ^16.0.0 ",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.0.0",
    "preconstruct": "^1.0.0",
    "prettier": "^3.2.2",
    "rollup": "^4.9.5",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-esbuild": "^6.1.0",
    "rollup-plugin-filesize": "^10.0.0",
    "rollup-plugin-typescript2": "^0.36.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "axios": "^1.6.5"
  }
}
